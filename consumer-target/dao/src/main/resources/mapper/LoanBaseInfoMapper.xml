<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wzx.demo.controller.LoanBaseInfoMapper" >
    <resultMap id="BaseResultMap" type="com.wzx.demo.LoanBaseInfo">
        <result column="loanno" property="loanNo" jdbcType="VARCHAR" />
        <result column="customer_code" property="customerCode" jdbcType="VARCHAR" />
        <result column="customer_name" property="customerName" jdbcType="VARCHAR" />
        <result column="amount" property="amount" jdbcType="DECIMAL" />
    </resultMap>
    <sql id="Base_Column_List">
        loanno,customer_code,customer_name,amount
    </sql>

    <select id="find" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
          <include refid="Base_Column_List"/>
        from loan_base_info
        where 1-1 and loanno=#{loanno}
    </select>





</mapper>

